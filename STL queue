#include<iostream>
#include<queue>
using namespace std;
int main(){
    //declare a vector
    queue<string> q;
    //add the data
    q.push("abc");
    q.push("def");
    q.push("ijk");
    q.push("ABC");
    //delete a data
    q.pop();
    //front data
    cout<<q.front()<<"\n";
    //print the size
    cout<<q.size();
    //print last data
    cout<<q.back();
    //print all the data
    while(q.empty()!=1){
        cout<<q.front();
        q.pop();
    }
    return 0;
}//github.com/Anand-29

#include<iostream>
#include<list>
using namespace std;
int main(){
    //declare a vector
    list<string> l;
    //add the data
    l.push_back("abc");
    l.push_back("def");
    l.push_back("ijk");
    l.push_front("ABC");
    //delete a data
    l.pop_back();
    l.pop_front();
    //front data
    cout<<l.front()<<"\n";
    cout<<l.back()<<"\n";
    for(string i: l)
        cout<<i<<",";
    return 0;
}//github.com/Anand-29

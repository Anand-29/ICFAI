#include <iostream>
#include<stack>
using namespace std;
int main()
{
    stack<string> s;
    //Add a data
    s.push("abc");
    s.push("def");
    //delete a data
    s.pop();
    //print stack top data
    cout<<s.top();
    //check if stack is empty or not(returns 1 if empty)
    if(s.empty())
        cout<<"empty stack";
    else
        cout<<"not empty stack";
    return 0;
}
